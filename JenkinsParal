pipeline {
   agent {label 'javanode'} 
   stages {
        stage('ParallelJobs') {
          parallel{
           stage{'Stage-1A'}{
             steps{
                sh 'sleep 120'
             }
           }
            stage{'Stage-1A'}{
             steps{
                sh 'sleep 120'
             }
           }
          }  
            }

              stage('Deploy') {
            steps {
                echo 'sh mkdir javanode1'
            }
        }
    }  
    }

