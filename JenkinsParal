pipeline {
   agent none  
   stages {
        stage('Stage1') {
         agent {label 'master' }
            steps {
                    sh 'sleep 60'
                    echo 'sh mkdir master-1'
            }
        }
       stage('Deploy') {
          agent{label 'javanode'}
            steps {
                sh 'sleep 60'
                echo 'sh mkdir javanode1'
            }
        }
    }
}
