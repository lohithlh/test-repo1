pipeline {
   agent {label 'javanode'}     
  stages {
        stage('checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/lohithlh/test-repo1.git'
            }
        }
        stage('Static-test') {
            steps {
            }    echo 'Running static tests on code'
        }
         stage('Build') {
            steps {
                sh 'mvn clean package'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying into Dev environment' 
                echo 'rsync'
            }
        }
    }
}
